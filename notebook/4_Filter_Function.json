					},
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"#2 - Using multilpe filter conditions \r\n",
					"# AND (&) , OR ( |) , NOT (~)\r\n",
					"df.filter((df['Labor Force'] > 50000) & (df['Labor Force'] < 80000)  ).count()"
				],
				"execution_count": 7
