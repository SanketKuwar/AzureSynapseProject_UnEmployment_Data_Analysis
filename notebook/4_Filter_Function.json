				"source": [
					"# 1 - filter where Labor fource > 50000\r\n",
					"\r\n",
					"df.filter(df['Labor Force'] > 50000).count()"
				],
				"execution_count": 5
			},
			{
				"cell_type": "code",
				"metadata": {
					"jupyter": {
						"source_hidden": false,
