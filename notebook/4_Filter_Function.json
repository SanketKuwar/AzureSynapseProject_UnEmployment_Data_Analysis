					}
				},
				"source": [
					"#2 - Filter rows based on string conditions\r\n",
					"from pyspark.sql.functions import col\r\n",
					"\r\n",
					"df.filter(col('Education Level').contains('degree')).count()"
				],
				"execution_count": 9
			},
